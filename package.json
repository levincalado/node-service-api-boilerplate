{
  "name": "node-service-api-boilerplate",
  "version": "1.0.0",
  "description": "DDD/Clean Architecture inspired boilerplate for Node web APIs",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node cluster.js",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon",
    "dev:sls": "sls offline -s local",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:features",
    "test:unit": "cross-env NODE_PATH=. NODE_ENV=test mocha --opts test/mocha.opts.unit",
    "test:features": "cross-env NODE_PATH=. NODE_ENV=test mocha --opts test/mocha.opts.features",
    "coverage": "cross-env NODE_PATH=. NODE_ENV=test nyc mocha --opts test/mocha.opts.unit",
    "lint": "eslint {src,config,test}/**/*.js",
    "console": "cross-env NODE_PATH=. node src/interfaces/console/index.js",
    "postinstall": "cd ./src/core-libs && git pull origin master && npm install && cd ../..",
    "sequelize:usersdb:migrate": "sequelize --options-path ./.sequelize-usersdb --env usersdb db:migrate",
    "sequelize:usersdb:migrate:undo": "sequelize --options-path ./.sequelize-usersdb --env usersdb db:migrate:undo",
    "sequelize:usersdb:seed:all": "sequelize --options-path ./.sequelize-usersdb --env usersdb db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:agaphetos/node-service-api-boilerplate.git"
  },
  "author": "Levin Calado <levincalado@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "awilix": "^4.2.5",
    "awilix-express": "^3.0.0",
    "body-parser": "^1.19.0",
    "chance": "^1.1.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "http-status": "^1.4.2",
    "inflection": "^1.12.0",
    "js-yaml": "^3.13.1",
    "knex": "^0.20.11",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "pm2": "^4.4.0",
    "sequelize": "^5.21.5",
    "serverless-http": "^2.3.2",
    "structure": "^1.8.0",
    "swagger-ui-express": "^4.1.3",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-change": "^2.1.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.20.1",
    "factory-girl": "^4.0.0",
    "listr": "^0.11.0",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "replace-in-file": "^5.0.2",
    "sequelize-cli": "^6.2.0",
    "serverless-offline": "^5.10.1",
    "supertest": "^3.4.2",
    "supertest-as-promised": "^4.0.2"
  }
}
